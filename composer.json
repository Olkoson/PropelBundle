{
  "name": "propel/propel-bundle",
  "description": "Integration of Propel in Symfony",
  "keywords": ["propel", "orm", "persistence"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [{
    "name": "William Durand",
    "email": "william.durand1@gmail.com"
  }],

  "autoload": {
    "psr-4": { "Propel\\Bundle\\PropelBundle\\": "" },
    "files": ["autoload_aliases.php"],
    "exclude-from-classmap": [ "Tests/" ]
  },
  "autoload-dev": {
    "psr-4": { "Propel\\Bundle\\PropelBundle\\Tests\\": "Tests/" }
  },
  "require": {
    "php": "^7.2.5",
    "ext-SimpleXML": "*",
    "propel/propel1": "^1.6.8",
    "symfony/framework-bundle": "^4.4|^5.0",
    "symfony/config": "^4.4|^5.0",
    "symfony/console": "^4.4|^5.0",
    "symfony/dependency-injection": "^4.4|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0|^9.0",
    "phing/phing": "^2.12",
    "sensio/framework-extra-bundle": "^5.0",
    "fzaninotto/faker": "^1.5",
    "twig/twig": "^1.34|^2.12",
    "symfony/form": "^4.4|^5.0",
    "symfony/security-core": "^4.4|^5.0",
    "symfony/yaml": "^4.4|^5.0",
    "symfony/translation": "^4.4|^5.0"
  },

  "conflict": {
    "symfony/propel1-bridge": ">=2.8.0"
  },

  "replace": {
    "symfony/propel1-bridge": "<=2.8.0"
  },

  "suggest": {
    "propel/propel-acl-bundle": "For using the Propel ACL implementation"
  }
}
